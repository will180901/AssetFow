    background-color: rgba(239, 68, 68, 0.2);
    color: #ef4444;
    border: 1px solid rgba(239, 68, 68, 0.3);
    border-radius: 8px;
    padding: 3px 6px;
    min-width: 55px;
    max-width: 105px;
    text-align: center;
    font-weight: 500;
    font-size: 6pt;
}

QLabel.MontantBadgeNeutre {
    background-color: rgba(107, 114, 128, 0.2);
    color: #6b7280;
    border: 1px solid rgba(107, 114, 128, 0.3);
    border-radius: 8px;
    padding: 3px 6px;
    min-width: 55px;
    max-width: 105px;
    text-align: center;
    font-weight: 500;
    font-size: 6pt;
}

QLabel.MontantBadgePending {
    background-color: rgba(245, 158, 11, 0.2);
    color: #f59e0b;
    border: 1px solid rgba(245, 158, 11, 0.3);
    border-radius: 8px;
    padding: 3px 6px;
    min-width: 55px;
    max-width: 105px;
    text-align: center;
    font-weight: 500;
    font-size: 6pt;
}

QLabel.MontantBadgeCritique {
    background-color: rgba(239, 68, 68, 0.9);
    color: white;
    border: 1px solid #ef4444;
    border-radius: 12px;
